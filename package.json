{
  "name": "next-fullstack-starter",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build:2-next": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.15.2",
    "@tailwindcss/forms": "^0.5.1",
    "@tippyjs/react": "^4.2.6",
    "@trpc/client": "^9.23.2",
    "@trpc/next": "^9.23.2",
    "@trpc/react": "^9.23.2",
    "@trpc/server": "^9.23.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.182",
    "autoprefixer": "^10.4.7",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "next": "^12.1.6-canary.16",
    "next-auth": "^4.3.4",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.7.5",
    "password-meter": "^3.9.3",
    "postcss": "^8.4.13",
    "react": "^18.1.0",
    "react-country-flag": "^3.0.2",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.0",
    "superjson": "^1.9.1",
    "tailwindcss": "^3.0.24",
    "zod": "^3.16.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.6",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "add": "^2.0.6",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": ">=8.0.1",
    "lint-staged": ">=12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prisma": "^3.15.2",
    "ts-node": "^10.7.0",
    "typescript": "4.6.4",
    "yarn": "^1.22.19"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix"
  },
  "license": "MIT"
}
